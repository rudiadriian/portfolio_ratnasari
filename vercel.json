{
    "version": 2,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.0" }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(favicon.ico|robots.txt|foto_ratna.png|.*\\.jpeg)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
